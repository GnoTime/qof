
prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: QOF
Description: QOF Query Object Framework Library.
Requires: glib-2.0 
Version: 0.5.2
# The libqof.a isn't supposed to have aversion number 
# Libs: -L${libdir} -lqof-0.5.2 -lqofsql-0.5.2
Libs: -L${libdir} -lqof -lqofsql
Cflags: -I${includedir} 
